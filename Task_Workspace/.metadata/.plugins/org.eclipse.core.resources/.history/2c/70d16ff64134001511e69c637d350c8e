	Context context;
	int[] flag;

	public ViewPagerAdapter(Context context, int[] flag) {

		this.context = context;
		this.flag = flag;

	}

	@Override
	public int getCount() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public boolean isViewFromObject(View arg0, Object arg1) {
		// TODO Auto-generated method stub
		return false;
	}

}
