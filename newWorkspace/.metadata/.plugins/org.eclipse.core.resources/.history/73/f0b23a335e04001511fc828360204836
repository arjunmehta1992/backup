package com.example.relative_layout_demo;

import java.sql.Date;
import java.text.SimpleDateFormat;

import com.helloworld.R;

import android.app.Activity;
import android.content.Intent;
import android.os.Bundle;
import android.view.animation.AnimationUtils;
import android.widget.Gallery;
import android.widget.ImageSwitcher;
import android.widget.TextView;
import android.widget.ViewSwitcher.ViewFactory;

public class Activity_FifthScreen extends Activity {

	ImageSwitcher imgSwitcher;

	int imgs[] = { R.drawable.two, R.drawable.six, R.drawable.five

	};

	@Override
	protected void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.activity_fifth_screen);
		imgSwitcher = (ImageSwitcher) findViewById(R.id.imageSwitcher1);

		imgSwitcher.setFactory((ViewFactory) this);
		imgSwitcher.setInAnimation(AnimationUtils.loadAnimation(this,
				android.R.anim.fade_in));

		imgSwitcher.setOutAnimation(AnimationUtils.loadAnimation(this,
				android.R.anim.fade_out));

		Gallery gallery = (Gallery) findViewById(R.id.gallery1);

		
		

		Intent intent = getIntent(); // gets the previously created intent
		String value = intent.getStringExtra("Text");

		

	}

}
